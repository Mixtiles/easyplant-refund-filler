{
  "author": "Ron Hagafny",
  "background": {
    "scripts": ["browser-polyfill.js", "util.js", "background.js"]
  },
  "browser_action": {
    "browser_style": false,
    "default_icon": "128X128.png",
    "default_popup": "options/options.html",
    "default_title": "Easyplant Refund Filler"
  },
  "description": "Enter Easyplant shopify tags into input fields by using autocomplete.",
  "homepage_url": "https://github.com/sblask/webextension-simple-form-fill",
  "icons": {
    "16": "16X16.png",
    "32": "32X32.png",
    "64": "64X64.png",
    "128": "128X128.png"
  },
  "manifest_version": 2,
  "name": "Easyplant Refund Filler",
  "options_ui": {
    "page": "options/options.html"
  },
  "permissions": ["<all_urls>", "tabs", "storage"],
  "version": "2.2.1"
}
